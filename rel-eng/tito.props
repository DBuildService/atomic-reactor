[buildconfig]
builder = osbsbuilder.AtomicReactorBuilder
tagger = tito.tagger.VersionTagger
changelog_do_not_remove_cherrypick = 0
changelog_format = %s (%ae)
tag_format = {version}
lib_dir = rel-eng/lib

[version_template]
destination_file = ./atomic_reactor/version.py
template_file = ./rel-eng/version.template
